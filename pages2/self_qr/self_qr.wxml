<!--pages/qrcode/qrcode.wxml-->
<view class="box  ">
    <!-- <view class='title'>{{title}}</view> -->
    <view class="cu-bar solid-bottom ">
        <view class="action">
            <text class="cuIcon-title text-black "></text>
            <text class=" text-black ">核销人员 绑定二维码</text>
        </view>
    </view>
    <view class="text-black text-bold text-center margin-top"><open-data type="userNickName" ></open-data></view>
    <view class="page-flex-center">
        <view class="qr">
            <qrcode placeholder="{{userSession}}"></qrcode>
        </view>
    </view>
    <view class="flex justify-center">
        <button class="cu-btn round bg-blue text-white" catchtap="reLogin">绑定成功后，点击重新认证</button>
    </view>
</view>